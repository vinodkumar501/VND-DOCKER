install docker 

 curl -fsSL https://get.docker.com -o get-docker.sh
 sudo sh get-docker.sh
 
 run

docker pull rabbitmq:3-management. ...
docker run --rm -it -p 15672:15672 -p 5672:5672 rabbitmq:3-management. ...
mkdir -p rabbitmq/rabbitmq-producer mkdir -p rabbitmq/rabbitmq-consumer. ...
cd rabbitmq/rabbitmq-producer npx express-generator npm install npm install amqplib

console

http://**.**.191:15672/
